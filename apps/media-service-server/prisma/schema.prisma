datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model PhotoDevice {
  brand       String?
  createdAt   DateTime  @default(now())
  description String?
  id          String    @id @default(cuid())
  name        String?
  price       Float?
  releaseDate DateTime?
  updatedAt   DateTime  @updatedAt
}

model BlogPost {
  author        String?
  content       String?
  createdAt     DateTime  @default(now())
  id            String    @id @default(cuid())
  publishedDate DateTime?
  title         String?
  updatedAt     DateTime  @updatedAt
}
